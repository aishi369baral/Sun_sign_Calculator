<%- include("partials/header") -%>

<h1 class="homeHeading">
    <%=heading%>
</h1>
<p class="homeParagraph"> <%=question%> <br> <%=paragraph%></p>

<!--Input box-->

<form class="row g-3 inputBox" action="/" method="POST">
    <div class="col-auto">
      <label for="" class="visually-hidden"></label>
      <input type="text" readonly class="form-control-plaintext" id="" value="Enter Date Of Birth">
    </div>
    <div class="col-auto">
      <label for="" class="visually-hidden"></label>
      <input type="number" class="form-control" id="" name="date" placeholder="eg. 24....">
    </div>
    <div class="col-auto">
        <label for="staticEmail2" class="visually-hidden"></label>
        <input type="text" readonly class="form-control-plaintext" id="staticEmail2" value="Enter Month of Birth">
      </div>
      <div class="col-auto">
        <label for="inputPassword2" class="visually-hidden"></label>
        <input type="text" class="form-control" id="inputPassword2" name="month" placeholder="eg. April....">
      </div>
    <div class="col-auto">
      <button type="submit" class="btn btn-info mb-3">Let's Find !</button>
    </div>
  </form>

  <h2 class="second-question"><%=secondQuestion%></h2>
  <h5 class="third-question"><%=thirdQuestion%></h5>
  <p class="second-paragraph"><%=secondParagraph%></p>
  <h5 class="fourth-question"><%=fourthQuestion%></h5>
  <p class=" third-paragraph"><%= thirdParagraph%></p>
<%- include("partials/footer") -%>